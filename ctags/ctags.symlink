--recurse=yes
--tag-relative=yes
--languages=C,C++,Python,Sh
--exclude=.git
--exclude=build
--exclude=dist
--python-kinds=-iv
--totals=yes
--verbose=yes
--langdef=Go
--langmap=Go:.go
--regex-Go=/func([ \t]+\([^)]+\))?[ \t]+([a-zA-Z0-9_]+)/\2/f,func/
--regex-Go=/var[ \t]+([a-zA-Z_][a-zA-Z0-9_]+)/\1/v,var/
--regex-Go=/type[ \t]+([a-zA-Z_][a-zA-Z0-9_]+)/\1/t,type/
